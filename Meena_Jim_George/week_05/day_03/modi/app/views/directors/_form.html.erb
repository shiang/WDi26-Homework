<%= form_for @director do |f|%>
<%= f.label :name %>
<%= f.text_field :name, :autofocus =>true%>
<%= f.label :nationality%>
<%= f.text_field :nationality%>
<%= f.label :dob, 'Date of birth'%>
<%=f.text_field :dob %>
<%=f.label :moviecount  %>
<%= f.text_field :moviecount%>

<%= f.label :image%>
<%= f.url_field :image%>
<%= f.submit %>

<%end%>
