<h2>New Cat</h2>

<form action="/cats_main" method="post">
  <label>Name:</label>
  <input type="text" name="name" placeholder="George">
  <label>Breed:</label>
  <input type="text" name="breed" placeholder="Persian">
  <label>Date Of Birth:</label>
  <input type="text" name="dob" placeholder="7th Jan">
  <label>Photo of Cat:</label>
  <input type="url" name="image" placeholder="http://...">
  <button>Add Cat!</button>
</form>
