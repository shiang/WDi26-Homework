<h2>Create your own console!</h2>

<form action="/consoles" method="post">
  <label>Name:</label>
  <input type="text" name="name" placeholder="Playstation 5">
  <label>Company:</label>
  <input type="text" name="company" placeholder="Nintendo">
  <label>Image:</label>
  <input type="url" name="image" placeholder="http://...">
  <button>Create</button>
</form>
