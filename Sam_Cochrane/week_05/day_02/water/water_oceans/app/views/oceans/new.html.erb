<h2>New Ocean</h2>
<form action="/oceans" method="post">
  <label>Name:</label>
  <input type="text" name="ocean[name]" placeholder="Ocean">

  <label>Image:</label>
  <input type="url" name="ocean[image]" placeholder="http://...">

  <label>Area:</label>
  <input type="number" name="ocean[area]" placeholder="%">

  <label>Volume:</label>
  <input type="number" name="ocean[volume]" placeholder="%">

  <label>Depth:</label>
  <input type="number" name="ocean[depth]" placeholder="m">

  <label>Coasline:</label>
  <input type="number" name="ocean[coastline]" placeholder="km">

  <button>Create ocean</button>
</form>
